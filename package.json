{
    "name": "orion-programmers-wordle",
    "version": "1.0.0",
    "description": "A programmer's Wordle game with daily coding challenges",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "generate-challenges": "node scripts/generateChallenges.js",
        "generate": "node generateChallenges.js",
        "generate-ai": "node generateChallenges.js --ai",
        "schedule-challenges": "node scripts/scheduleChallenges.js",
        "view-challenges": "cd scripts && npm run view",
        "activate-challenges": "cd scripts && npm run activate",
        "test-generator": "cd scripts && npm run test"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-mongo-sanitize": "^2.2.0",
        "express-rate-limit": "^7.1.5",
        "helmet": "^7.1.0",
        "jsonwebtoken": "^9.0.2",
        "mongoose": "^8.0.3",
        "morgan": "^1.10.0",
        "node-cron": "^3.0.3",
        "openai": "^4.86.1",
        "xss-clean": "^0.1.4"
    },
    "devDependencies": {
        "nodemon": "^3.0.2"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
